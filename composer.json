        "webflo/drupal-core-require-dev": "~8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {"php": "7.0.30"}
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "@composer drupal:scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "@composer drupal:scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-types": ["library"],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "enyo/dropzone"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches":  {
            "drupal/quick_node_clone": {
                "2969141: Fatal error on admin/content in PHP 5.6": "https://www.drupal.org/files/issues/2018-05-10/quick_node_clone-clone_permission_parameter-2969141-2_0.patch"
            },
            "drupal/config_update": {
              "2961886: Unit test for Config Differ is failing": "https://www.drupal.org/files/issues/2018-04-17/2961886.patch"
            },
            "drupal/config_installer": {
              "2998264: Cast t() to string to fix travis based automated testing": "https://www.drupal.org/files/issues/2018-09-13/config-installer-2998264-2.patch"
            },
            "drupal/comment_notify": {
              "2865943: Notice: Trying to get property of non-object in comment_notify_form_user_form_alter()": "https://www.drupal.org/files/issues/2018-10-10/comment_notify-user-settings-defaults-2865943-15.patch"
            }
        }
    },
